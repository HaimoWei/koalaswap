{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowFileServiceUploads",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::143223323809:user/koalaswap-dev-uploader"
      },
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::koalaswap/files/*"
      ]
    }
  ]
}